{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/public/*": ["public/*"],
      "@/app/*": ["src/app/*"],
      "@/api/*": ["src/app/api/*"],
      "@/layout/*": ["src/components/layout/*"],
      "@/module/*": ["src/components/module/*"],
      "@/template/*": ["src/components/template/*"],
      "@/models/*": ["src/models/*"],
      "@/providers/*": ["src/providers/*"],
      "@/utils/*": ["src/utils/*"],
      "@/schema/*": ["src/schema/*"],
      "@/constants/*": ["src/constants/*"]
    }
  }
}
